{
    "name": "ev-travel-history-api",
    "license": "MIT",
    "main": "index.ts",
    "scripts": {
        "build": "npm run gql-generate",
        "ts-check": "tsc --noEmit ./airtableApi/**/*.ts",
        "gql-generate": "apollo client:codegen --localSchemaFile=test/schema.graphql --target typescript --includes=test/**/*.ts --no-addTypename --globalTypesFile=test/__generated__/global.d.ts --tsFileExtension=d.ts __generated__"
    },
    "dependencies": {
        "apollo": "^2.33.2",
        "apollo-server": "^2.24.1",
        "apollo-server-lambda": "^2.24.1",
        "bufferutil": "^4.0.3",
        "encoding": "^0.1.13",
        "utf-8-validate": "^5.0.5"
    },
    "devDependencies": {
        "@types/node": "^15.6.0",
        "prettier": "^2.3.0",
        "typescript": "^4.2.4"
    }
}
