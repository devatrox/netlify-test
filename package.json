{
    "name": "ev-travel-history",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "netlify-lambda install functions/ && npm run build:functions",
        "build:functions": "cd functions/ && npm run build && cd ..",
        "netlify": "netlify dev"
    },
    "eslintConfig": {
        "extends": [
            "plugin:prettier/recommended"
        ]
    },
    "prettier": {
        "singleQuote": false,
        "semi": true,
        "trailingComma": "all"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@netlify/functions": "^0.7.2",
        "netlify-lambda": "^2.0.7"
    },
    "devDependencies": {
        "@types/node": "^15.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "prettier": "^2.3.0",
        "typescript": "^4.2.4"
    }
}
